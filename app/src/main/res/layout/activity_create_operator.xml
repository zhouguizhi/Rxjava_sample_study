<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:id="@+id/activity_create_operator_tv_create_operator"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:text="create操作符"
                android:gravity="center"
                android:background="@color/colorAccent"
                />
            <TextView
                android:id="@+id/activity_create_operator_tv_defer_operator"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:text="defer操作符"
                android:gravity="center"
                android:background="@color/colorAccent"
                android:layout_marginTop="10dp"
                />
            <TextView
                android:id="@+id/activity_create_operator_tv_empty_operator"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:text="empty操作符"
                android:gravity="center"
                android:background="@color/colorAccent"
                android:layout_marginTop="10dp"
                />
            <TextView
                android:id="@+id/activity_create_operator_tv_error_operator"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:text="error操作符"
                android:gravity="center"
                android:background="@color/colorAccent"
                android:layout_marginTop="10dp"
                />
            <TextView
                android:id="@+id/activity_create_operator_tv_from_operator"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:text="from操作符"
                android:gravity="center"
                android:background="@color/colorAccent"
                android:layout_marginTop="10dp"
                />
            <TextView
                android:id="@+id/activity_create_operator_tv_just_operator"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:text="just操作符"
                android:gravity="center"
                android:background="@color/colorAccent"
                android:layout_marginTop="10dp"
                />
            <TextView
                android:id="@+id/activity_create_operator_tv_timer_operator"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:text="timer操作符"
                android:gravity="center"
                android:background="@color/colorAccent"
                android:layout_marginTop="10dp"
                />
            <TextView
                android:id="@+id/activity_create_operator_tv_range_operator"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:text="range操作符"
                android:gravity="center"
                android:background="@color/colorAccent"
                android:layout_marginTop="10dp"
                />
            <TextView
                android:id="@+id/activity_create_operator_tv_never_operator"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:text="never操作符"
                android:gravity="center"
                android:background="@color/colorAccent"
                android:layout_marginTop="10dp"
                />
            <TextView
                android:id="@+id/activity_create_operator_tv_interval_operator"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:text="interval操作符"
                android:gravity="center"
                android:background="@color/colorAccent"
                android:layout_marginTop="10dp"
                />
            <TextView
                android:id="@+id/activity_create_operator_tv_generate_operator"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:text="generate操作符"
                android:gravity="center"
                android:background="@color/colorAccent"
                android:layout_marginTop="10dp"
                />

        </LinearLayout>
    </ScrollView>
</LinearLayout>